<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RuntimeIdentifiers>win-x86;win-x64;win-arm</RuntimeIdentifiers>
		<Product>Work-Time Watch-Dog</Product>
		<Authors>Peter Hra≈°ko</Authors>
		<Company>Open source community</Company>
		<Copyright>Practically copyleft</Copyright>
		<AssemblyName>wtwd</AssemblyName>
		<NoWarn>1701;1702;AD0001;NETSDK1182</NoWarn>
		<PackageId></PackageId>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DebugType>none</DebugType>
		<DebugSymbols>false</DebugSymbols>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="CommandLineParser" Version="2.9.1" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\wtwd.cli.List\wtwd.cli.List.csproj" />
	  <ProjectReference Include="..\wtwd.cli.Lock\wtwd.cli.Lock.csproj" />
	  <ProjectReference Include="..\wtwd.cli.Unlock\wtwd.cli.Unlock.csproj" />
	  <ProjectReference Include="..\wtwd.model\wtwd.model.csproj" />
	  <ProjectReference Include="..\wtwd.utilities\wtwd.utilities.csproj" />
	  <ProjectReference Include="..\wtwd.xform\wtwd.xform.csproj" />
	</ItemGroup>

	<Target Name="PostClean" AfterTargets="Clean">
		<RemoveDir Directories="$(BaseIntermediateOutputPath)" /><!-- obj -->
		<RemoveDir Directories="$(BaseOutputPath)" /><!-- bin -->
	</Target>
</Project>
